{
  "expo": {
    "name": "reactNativeCrashCourse",
    "slug": "reactNativeCrashCourse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/blog.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash":{
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "red"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "red"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
     
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
